                <br> <br>
                <div class="col s12">
                    <img width="1024"  src="{{ asset('data/img/contact.jpg') }}" alt="">
                    <table>
                        <thead>
                        <th style="font-size: 150%"> {% trans from 'contact' %} contact.cameroun {% endtrans %} </th>

                        </thead>
                        <tbody>
                        <tr>
                            <td>

                                <p><b>{% trans from 'contact' %} contact.ville1 {% endtrans %}</b><i>{% trans from 'contact' %} contact.ville1_content {% endtrans %}</i></p>
