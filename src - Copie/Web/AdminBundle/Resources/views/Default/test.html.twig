{% extends 'AdminBundle::layout.html.twig' %}
{% block meta %}
    <meta name = "{% trans from 'refer' %} home.title {% endtrans %}" >
    <meta name = "keywords" content = "{% trans from 'refer' %} home.description {% endtrans %}" >
{% endblock %}
{% block title %}
    Semantica Technologies Corporation - STC - Admin
{% endblock %}

{% block mycss %}
    <link rel="stylesheet" href="{{ asset('components/cssbase/datatablehelp.css') }}">
    <link rel="stylesheet" href="{{ asset('components/cssbase/datatablehelp2.css') }}">
    <style>

    </style>
{% endblock %}
{% block header %}
    {{ parent() }}

{% endblock %}
{% block  main %}
    <div class="row">
        <div class="col l12">
            <div class="card z-depth-1">
                <div class="card-content">
                    <span class="card-title"></span><br/><br/>
                    <table class="centered bordered responsive-table" id="table">
                        <thead>
                        <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th>Age</th>
                            <th>Address</th>
                            <th>Designation</th>
                            <th>Department</th>
                            <th>Salary</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for item in 1..10 %}
                            <tr >
                                <td>{{item}}</td>
                                <td>{{item}}</td>
                                <td>{{item}}</td>
                                <td>{{item}}</td>
                                <td>{{item}}</td>
                                <td>{{item}}</td>
                                <td>{{item}}</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
{% endblock %}

{% block myjs %}
    <script>
        $(document).ready(function() {
            $('#table').dataTable();
            $('select').material_select();
        } );
    </script>
{% endblock %}
